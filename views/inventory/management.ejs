<div class="form-container inventory">
  <h1><%= title %></h1>

  <% if (typeof message !== 'undefined' && message) { %>
  <div class="flash-message">
    <p><%= message %></p>
  </div>
  <% } %> <% if (typeof errors !== 'undefined' && errors) { %>
  <ul class="notice">
    <% errors.array().forEach(error => { %>
    <li><%= error.msg %></li>
    <% }) %>
  </ul>
  <% } %>

  <div class="form-card-inventory">
    <a class="inv-link" href="/inv/add-classification">Add new Classification</a>
    <a class="inv-link" href="/inv/add-vehicle">Add new Vehicle</a>
  </div>
</div>
