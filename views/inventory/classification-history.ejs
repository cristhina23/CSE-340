<h1><%= title %></h1>
<table class="history-table">
  <thead>
    <tr>
      <th>Classification Name</th>
      <th>Action</th>
      <th>User</th>
      <th>Date</th>
    </tr>
  </thead>
  <tbody>
    <% history.forEach(row => { %>
      <tr>
        
        <td><%= row.classification_name %></td>

       
        <td><%= row.action %></td>

        
        <td>
          <%= (row.account_firstname || row.account_lastname) 
                ? ((row.account_firstname || '') + ' ' + (row.account_lastname || '')) 
                : 'Unknown' %>
        </td>

        
        <td><%= row.change_date ? new Date(row.change_date).toLocaleString() : '' %></td>
      </tr>
    <% }) %>
  </tbody>
</table>
